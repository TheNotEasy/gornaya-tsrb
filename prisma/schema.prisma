datasource db {
    provider = "postgresql"
}

generator client {
    provider = "prisma-client"
    output = "../generated"
}

model GalleryImage {
    id Int @id @default(autoincrement())
    url String @unique
    caption String
}

model PaymentRequest {
    id Int @id @default(autoincrement())
    attachment String @unique
    accepted Boolean @default(false)
}

model Admin {
    id Int @id @default(autoincrement())
}